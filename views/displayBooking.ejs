<link rel="stylesheet" type="text/css" href="/css/home.css">
<div class="home-body">
    <h1>Welcome Page</h1>

    <h2>Identity Service</h2>
    <h4>Show all bookings</h4>

    <div class="user-list">
        <%var count=1;%>
        <%for(bookings of bookings){%>
            <%var date = new Date(bookings.date);%>
            <div id="user">
                <p id="count"><%=count%></p>
                <p class="shift"><%=bookings.user1%></p>
                <p class="shift"><%=bookings.user2%></p>
                <p class="shift"><%=bookings.purpose%></p>
                <p class="shift"><%= date.toDateString(); %></p>
                <p class="shift"><%=bookings.time1%></p>
                <p class="shift"><%=bookings.time2%></p>
                <%count++;%>
            </div>
        <%}%>
    </div>
    </div>

